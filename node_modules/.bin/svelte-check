#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/santi/Downloads/TESTDASH/shadcn-svelte/node_modules/.pnpm/svelte-check@3.8.5_postcss-load-config@5.1.0_jiti@1.21.6_postcss@8.5.3_tsx@3.14.0__postcss@8.5.3_svelte@4.2.19/node_modules/svelte-check/bin/node_modules:/Users/santi/Downloads/TESTDASH/shadcn-svelte/node_modules/.pnpm/svelte-check@3.8.5_postcss-load-config@5.1.0_jiti@1.21.6_postcss@8.5.3_tsx@3.14.0__postcss@8.5.3_svelte@4.2.19/node_modules/svelte-check/node_modules:/Users/santi/Downloads/TESTDASH/shadcn-svelte/node_modules/.pnpm/svelte-check@3.8.5_postcss-load-config@5.1.0_jiti@1.21.6_postcss@8.5.3_tsx@3.14.0__postcss@8.5.3_svelte@4.2.19/node_modules:/Users/santi/Downloads/TESTDASH/shadcn-svelte/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/santi/Downloads/TESTDASH/shadcn-svelte/node_modules/.pnpm/svelte-check@3.8.5_postcss-load-config@5.1.0_jiti@1.21.6_postcss@8.5.3_tsx@3.14.0__postcss@8.5.3_svelte@4.2.19/node_modules/svelte-check/bin/node_modules:/Users/santi/Downloads/TESTDASH/shadcn-svelte/node_modules/.pnpm/svelte-check@3.8.5_postcss-load-config@5.1.0_jiti@1.21.6_postcss@8.5.3_tsx@3.14.0__postcss@8.5.3_svelte@4.2.19/node_modules/svelte-check/node_modules:/Users/santi/Downloads/TESTDASH/shadcn-svelte/node_modules/.pnpm/svelte-check@3.8.5_postcss-load-config@5.1.0_jiti@1.21.6_postcss@8.5.3_tsx@3.14.0__postcss@8.5.3_svelte@4.2.19/node_modules:/Users/santi/Downloads/TESTDASH/shadcn-svelte/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
